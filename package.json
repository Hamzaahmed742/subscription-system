{
  "name": "subscription-system-nestjs-service",
  "private": true,
  "version": "1.0.0",
  "description": "Subscription System",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "precommit": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 lint-staged",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "format": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 prettier **/**/*.ts --ignore-path ./.prettierignore --write && git status",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --verbose --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "patch-package",
    "migrate": "ts-node migrate.ts",
    "gen:typings": "ts-node src/generate-typings.ts",
    "doc": "npx compodoc -p tsconfig.json",
    "doc:serve": "npm run doc && npx compodoc -s"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@godaddy/terminus": "^4.3.1",
    "@golevelup/nestjs-rabbitmq": "^1.15.1",
    "@nestjs/common": "^7.1.1",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.1.1",
    "@nestjs/graphql": "^7.4.6",
    "@nestjs/platform-express": "^7.1.1",
    "@nestjs/platform-fastify": "^7.2.0",
    "@nestjs/terminus": "^7.0.0",
    "@types/lodash": "^4.14.157",
    "@types/yup": "^0.29.3",
    "apollo-server-core": "^2.15.1",
    "apollo-server-fastify": "^2.15.1",
    "async-exit-hook": "^2.0.1",
    "bunyan": "^1.8.14",
    "bunyan-prettystream": "^0.1.3",
    "chalk": "^4.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "fastify-compress": "^2.0.1",
    "fastify-helmet": "^3.0.2",
    "graphql": "^15.2.0",
    "graphql-tools": "^6.0.12",
    "graphql-type-json": "^0.3.2",
    "http-json-errors": "^1.2.8",
    "inversify": "^5.0.1",
    "lodash": "^4.17.19",
    "nestjs-flub": "^0.2.0",
    "ravendb": "^4.1.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "type-graphql": "^1.0.0-rc.3",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.2.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.1.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "bluebird": "^3.7.2",
    "compodoc": "^0.0.41",
    "envalid": "^6.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "mkdirp": "^1.0.4",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.3",
    "umzug": "^2.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      ".module.ts",
      ".validation.ts",
      ".resolver.ts",
      "<rootDir>/dispatcher.ts",
      "<rootDir>/generate-typings.ts",
      "<rootDir>/graphql.schema.ts",
      "<rootDir>/main.ts",
      "<rootDir>/src/app/main.ts",
      ".seed.ts",
      "<rootDir>/config/",
      "<rootDir>/db/",
      "<rootDir>/shared/"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "yarn format",
      "yarn lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "amend-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  }
}
